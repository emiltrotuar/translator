{
  "manifest_version": 2,
  "name": "translator",
  "version": "1.0",

  "description": "Translates a selected text using Lingua Leo API",

  "icons": {
    "48": "notepad.png"
  },
  "permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["translator.js"],
      "run_at": "document_end"
    }
  ]

}
